S_datasets = read.csv("Salary_Data.csv")
# spliting the data_set
library(caTools)
set.seed(123)
to_split = sample.split(S_datasets$Salary,SplitRatio = 2/3)
training_set = subset(S_datasets,to_split == TRUE)
test_set = subset(S_datasets,to_split == FALSE)
# training(fitting) the data_set
simp_regressor = lm(formula = ' Salary ~ YearsExperience ' , data = training_set)
# Predicting the data_set
y_pred_s = predict(simp_regressor, newdata = test_set)
# visualizing
# Training set
library(ggplot2)
ggplot() +
geom_point(aes( x = training_set$YearsExperience, y = training_set$Salary),
colour = "Green")+
geom_line(aes( x = training_set$YearsExperience, y = predict(simp_regressor, newdata = training_set)),
colour = 'blue')+
ggtitle("Salary Prediction ( Training Set )")+
xlab("YearsExperience")+
ylab("Salary")
# Test set
library(ggplot2)
ggplot() +
geom_point(aes( x = training_set$YearsExperience, y = training_set$Salary),
colour = "Green")+
geom_point(aes( x = test_set$YearsExperience, y = test_set$Salary),
colour = "Red")+
geom_line(aes( x = training_set$YearsExperience, y = predict(simp_regressor, newdata = training_set)),
colour = 'blue')+
ggtitle("Salary Prediction ( Test Set )")+
xlab("YearsExperience")+
ylab("Salary")
